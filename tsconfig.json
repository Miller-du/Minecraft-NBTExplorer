{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "types": [
      "new-javascript"
    ],
    "paths": {
      "https://cdn.jsdelivr.net/npm/*": [
        "../node_modules/*"
      ],
      "https://cdn.jsdelivr.net/npm/nbtify*": [
        "../node_modules/nbtify"
      ],
      "https://cdn.jsdelivr.net/npm/@stardazed/streams-compression*": [
        "../node_modules/@stardazed/streams-compression"
      ]
    },
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": [
    "./service-worker.js"
  ]
}